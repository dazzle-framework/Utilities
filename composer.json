{
    "name": "dazzlesoftware/utilities",
    "type": "library",
    "description": "Dazzle Utilities Package",
    "keywords": ["dazzle", "framework", "utilities"],
    "homepage": "https://github.com/dazzle-framework/utilities",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "dazzlesoftware/string": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Utilities\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
